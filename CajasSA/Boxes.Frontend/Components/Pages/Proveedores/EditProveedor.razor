﻿@if (proveedor is null)
{
    <Loading />
}
else
{
    <MudDialog>
        <DialogContent>
            <ProveedorForm Proveedor="proveedor"
                          OnValidSubmit="EditAsync"
                          ReturnAction="Return"
                          @key="proveedor.Id" />
        </DialogContent>
    </MudDialog>
}
